<h2>Home Page</h2>
<div class="post-grid">
    <% posts.forEach(function(post) { %>
        <div class="post-card">
            <img src="<%= post.image %>" alt="<%= post.title %>" class="post-image">
            <!-- <h3><%= post.title %></h3>
            <h3><%= post.image  %></h3> -->
        </div>
    <% }); %>
</div>
